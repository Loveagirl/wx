<!--components/classic/music/music.wxml-->
<view class="classic-container" >
<image class="classic-img {{ playing ? 'rotation' : ''}} " src="{{imgSrc}}"  />
<image class="player-img" bind:tap="changeStatus" src="{{playing ? stop : play}}" />
<image class="tag" src="{{musicTagImg}}" />
<text class="text" >{{text}}</text>
</view>
